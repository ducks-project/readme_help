{
    "name": "drupal/readme_help",
    "description": "Provide service in order to easily parse README file from project.",
    "type": "drupal-module",
    "keywords": ["Drupal", "module", "help", "readme", "parser", "markdown", "service", "improvement"],
    "homepage": "https://github.com/ducks-project/readme_help",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Adrien Loyant",
            "email": "donaldinou@gmail.com",
            "homepage": "https://github.com/donaldinou",
            "role": "Developper"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.6",
        "friendsofphp/php-cs-fixer": "^1.12"
    },
    "conflict": {
        "drupal/drupal": "<8.0"
    },
    "support": {
        "email": "donaldinou@gmail.com"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\readme_help\\": "src"
        },
        "exclude-from-classmap": [
            "/src/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Drupal\\readme_help\\": "src/Tests"
        }
    }
}
